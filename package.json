    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "histoire": "^0.11.6",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.2.3",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "^0.11.4",
